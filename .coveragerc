[run]
source = .
omit =
    tests/*
    research/*
    monitoring/*
    archive/*
    venv/*
    .gemini/*
    get_schema.py
    verify_monetization.py
    verify_market.py
    test_styling.py
    
[report]
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
